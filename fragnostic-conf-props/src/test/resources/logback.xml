<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>fragnostic conf - [%4level] [%d{HH:mm:ss.SSS}] [%thread] %logger{4} : %m%n</pattern>
        </encoder>
    </appender>

    <!--logger name="com.fragnostic.dao" level="INFO"/-->

    <root level="warn">
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>